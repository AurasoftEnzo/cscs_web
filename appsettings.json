{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "CSCSConfig": {
    "PreprocessTokens": "include,startdebugger,function,csfunction,add_comp_namespace,add_comp_definition,dllfunction,dllsub,importdll",

    "SQLConnectionString": "Server=127.0.0.1,1433;Database=DATABSE_NAME;MultipleActiveResultSets=true;User Id=sa;Password=PASSWORD;TrustServerCertificate=true;",

    "ScriptsDirectory": "PATH_TO_PROJECT\\CSCS-web-1\\wwwroot\\scripts\\",
    "TemplatesDirectory": "PATH_TO_PROJECT\\CSCS-web-1\\wwwroot\\templates\\",

    "StaticFilesDirectory": "PATH_TO_PROJECT\\CSCS-web-1\\StaticFiles\\",
    
    "StartScript": "startScript.cscs"
  }
}
